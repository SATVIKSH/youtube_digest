{
    "builds": [{
        "src": "youtube_digest/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "functions":{
        "app/api/**/*": {
            "maxDuration": 60
          }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "youtube_digest/wsgi.py"
        }
    ]
}